{
  "manifest_version": 3,
  "name": "Buscar y clickear IndianWebs",
  "version": "1.0",
  "description": "Abre Google y hace clic en indianwebs.com",
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Buscar IndianWebs"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
  {
    "resources": ["content_sidebar.js", "img/imagen.png"],
    "matches": ["<all_urls>"]
  }
]
,
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  }
}
