{
  "manifest_version": 3,
  "name": "BeFound",
  "version": "2.0",
  "description": "Descubre en qué posición aparece tu web, haz clic automáticamente en tu dominio y mejora tu visibilidad con esta extensió SEO.",
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "BeFound"
  },
  "host_permissions": [
    "https://www.google.com/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [
  {
    "resources": ["content_sidebar.js", "img/imagen.png"],
    "matches": ["<all_urls>"]
  }
]
,
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  }
}
